    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import org.pentaho.platform.engine.core.system.PentahoSessionHolder;
import org.pentaho.platform.engine.core.system.PentahoSystem;
import org.pentaho.platform.api.engine.security.userroledao.IUserRoleDao;
import org.pentaho.platform.web.http.api.resources.RoleListWrapper;
import org.pentaho.platform.api.engine.ISecurityHelper;
import org.pentaho.platform.engine.security.SecurityHelper;
import java.net.URLEncoder;
import java.util.concurrent.Callable;
import java.util.List;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
	Callable callable = new Callable() {
		public Object call() throws Exception {
			IUserRoleDao roleDao = (IUserRoleDao)PentahoSystem.get( IUserRoleDao.class, "userRoleDaoProxy", PentahoSessionHolder.getSession() );
			if(roleDao == null) {
				logBasic("roledao is null");
			} 
			return new RoleListWrapper(roleDao.getUserRoles(null, getParameter("userName")));
        }
    };

	Object[] r = getRow();
	if(r == null) {
		setOutputDone();
		return false;
	}
	
	try {
		Object roles = SecurityHelper.getInstance().runAsSystem(callable);
		List a = ((RoleListWrapper)roles).getRoles();
		StringBuilder sb = new StringBuilder("");
		for(int i = 0; i < a.size(); i++){
			if(i>0) {
				sb.append("~");	
			}
			sb.append(URLEncoder.encode((String)a.get(i),"UTF-8"));
		}
		get(Fields.Out, "comparator").setValue(r, sb.toString());
		putRow(data.outputRowMeta, r);
	} catch (Exception e) {
	}

	return true;
}
]]></class_source>
